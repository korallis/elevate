NEXT_PUBLIC_APP_NAME=Elevate
NEXT_PUBLIC_APP_VERSION=0.0.0
NEXT_PUBLIC_API_BASE=http://localhost:3001

# Metadata DB (PostgreSQL 17)
# Example local dev URL; see repo root .env.example for PG vars
DATABASE_URL=postgres://sme:sme@localhost:5432/sme

# (Optional) SQLite fallback for quick local prototyping
# DATABASE_URL="file:./prisma/dev.db"

# iron-session configuration
# Must be at least 32 characters long
IRON_SESSION_PASSWORD="replace-with-32+char-strong-secret"
IRON_SESSION_COOKIE_NAME="elevate_session"
