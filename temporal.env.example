# Temporal Configuration
# Add these to your .env file or export them in your environment

# Temporal Server Connection
TEMPORAL_SERVER_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=sme-analytics

# Optional: For production with TLS
# TEMPORAL_TLS_CRT=path/to/client.pem
# TEMPORAL_TLS_KEY=path/to/client-key.pem
# TEMPORAL_TLS_CA=path/to/ca.pem
# TEMPORAL_TLS_SERVER_NAME=temporal

# Worker Configuration
ENABLE_HEALTH_CHECK=true
HEALTH_PORT=8080

# Development Configuration
NODE_ENV=development